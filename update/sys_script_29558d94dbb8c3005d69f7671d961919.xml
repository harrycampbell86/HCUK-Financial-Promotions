        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {

var startDt = new GlideDateTime(current.u_campaign_start);
  var endDt = new GlideDateTime(current.u_campaign_end);
  var start = startDt.getNumericValue();
  var end = endDt.getNumericValue();
  if (start > end) {
    
    current.u_campaign_end.setError('End date cannot be before start date');
    current.setAbortAction(true);
  }

})(current, previous);]]></script>
